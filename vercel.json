{
  "redirects": [
    {
      "source": "/(.*)",
      "has": [
        { "type": "host", "value": "devtechai.org" }
      ],
      "destination": "https://www.devtechai.org/$1",
      "permanent": true
    },
    {
      "source": "/(.*)",
      "has": [
        { "type": "header", "key": "x-forwarded-proto", "value": "http" }
      ],
      "destination": "https://www.devtechai.org/$1",
      "permanent": true
    }
  ]
}
